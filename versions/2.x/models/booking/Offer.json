{
   "type":"Offer",
   "derivedFrom":"https://schema.org/Offer",
   "isJsonLd":true,
   "idFormat":"https://schema.org/URL",
   "sampleId":"https://example.com/Offer/",
   "status":{
      "REQUEST":{
        "hasId":true,
         "requiredFields":[
            "@type",
            "@id",
            "availableChannel"
         ],
         "recommendedFields":[
            "ageRange"
         ],
         "requiredOptions":[

         ],
         "shallNotInclude":[
            "validFromBeforeStartDate",
            "latestCancellationBeforeStartDate",
            "name",
            "price",
            "priceCurrency"
         ]
      },
      "RESPONSE":{
         "hasId":true,
         "requiredFields":[
            "@type",
            "@id",
            "availableChannel",
            "price",
            "priceCurrency"
         ],
         "recommendedFields":[
            "name",
            "ageRange"
         ],
         "requiredOptions":[

         ]
      }
   },
   "inSpec":[
      "@type",
      "@id",
      "price",
      "priceCurrency",
      "name",
      "ageRange",
      "advanceBooking",
      "prepayment",
      "availableChannel",
      "validFromBeforeStartDate",
      "latestCancellationBeforeStartDate",
      "openBookingFlowRequirement"
   ],
   "notInSpec":[
      "acceptedPaymentMethod",
      "addOn",
      "advanceBookingRequirement",
      "aggregateRating",
      "areaServed",
      "availability",
      "availabilityEnds",
      "availabilityStarts",
      "availableAtOrFrom",
      "availableDeliveryMethod",
      "businessFunction",
      "category",
      "deliveryLeadTime",
      "eligibleCustomerType",
      "eligibleDuration",
      "eligibleQuantity",
      "eligibleRegion",
      "eligibleTransactionVolume",
      "gtin12",
      "gtin13",
      "gtin14",
      "gtin8",
      "includesObject",
      "ineligibleRegion",
      "inventoryLevel",
      "itemCondition",
      "itemOffered",
      "mpn",
      "offeredBy",
      "priceSpecification",
      "priceValidUntil",
      "review",
      "seller",
      "serialNumber",
      "sku",
      "validFrom",
      "validThrough",
      "warranty",
      "additionalType",
      "alternateName",
      "description",
      "disambiguatingDescription",
      "identifier",
      "image",
      "mainEntityOfPage",
      "potentialAction",
      "sameAs",
      "subjectOf",
      "url"
   ],
   "fields":{
      "@type":{
         "fieldName":"type",
         "requiredType":"https://schema.org/Text",
         "requiredContent":"Offer"
      },
      "@id":{
         "fieldName":"@id",
         "requiredType":"https://schema.org/URL"
      },
      "price":{
         "attributeName":"price",
         "requiredType":"https://schema.org/Float",
         "description":"The offer price available to participants, which includes or excludes tax depending on the taxMode of the Order."
      },
      "priceCurrency":{
         "attributeName":"priceCurrency",
         "requiredType":"https://schema.org/Text",
         "description":"The currency of the price. Specified as a 3-letter ISO 4217 value. If an Offer has a zero price, then this property is not required. Otherwise the priceCurrency must be specified."
      },
      "name":{
         "attributeName":"name",
         "requiredType":"https://schema.org/Text",
         "description":"The name of the Offer suitable for communication to participants."
      },
      "ageRange":{
         "attributeName":"ageRange",
         "model":"#QuantitativeValue",
         "description":"ndicates that an Offer is applicable to a specific age range. Specified as a QuantitativeValue with minValue and maxValue properties."
      },
      "advanceBooking":{
         "attributeName":"advanceBooking",
         "sameAs":"https://www.openactive.io/ns/#advanceBooking",
         "model":"#RequiredStatusType",
         "options":[
            "https://openactive.io/Required",
            "https://openactive.io/Optional",
            "https://openactive.io/Unavailable"
         ],
         "description":"Indicates whether to accept this offer, a participant must book in advance, whether they must pay on attending, or have option to do either. Values must be one of  https://openactive.io/Required,  https://openactive.io/Optional or  https://openactive.io/Unavailable."
      },
      "prepayment":{
         "attributeName":"prepayment",
         "sameAs":"https://www.openactive.io/ns/#prepayment",
         "model":"#RequiredStatusType",
         "options":[
            "https://openactive.io/Required",
            "https://openactive.io/Optional",
            "https://openactive.io/Unavailable"
         ],
         "description":"Indicates whether to accept this offer, a participant must pay in advance, pay when attending, or have the option to do either. Values must be one of  https://openactive.io/Required,  https://openactive.io/Optional or  https://openactive.io/Unavailable."
      },
      "availableChannel":{
         "attributeName":"availableChannel",
         "sameAs":"https://www.openactive.io/ns/#availableChannel",
         "requiredType":"ArrayOf#availableChannelType",
         "options":[
            "https://openactive.io/OpenBookingPrepayment",
            "https://openactive.io/TelephoneAdvanceBooking",
            "https://openactive.io/Optional",
            "https://openactive.io/TelephonePrepayment",
            "https://openactive.io/OnlinePrepayment"
         ]
      },
      "validFromBeforeStartDate":{
         "attributeName":"validFromBeforeStartDate",
         "sameAs":"https://www.openactive.io/ns/#validFromBeforeStartDate",
         "model":"#Duration",
         "description":"The duration before the startDate for which this Offer is valid, given in ISO 8601 format. This is a relatively-defined equivalent of schema:validFrom, to allow for Offer inheritance."
      },
      "latestCancellationBeforeStartDate":{
         "attributeName":"latestCancellationBeforeStartDate",
         "sameAs":"https://www.openactive.io/ns/#latestCancellationBeforeStartDate",
         "model":"#Duration",
         "description":"The duration before the startDate during which this Offer may not be cancelled, given in ISO 8601 format."
      },
      "openBookingFlowRequirement":{
         "attributeName":"openBookingFlowRequirement",
         "sameAs":"https://www.openactive.io/ns/#openBookingFlowRequirement",
         "model":"ArrayOf#OpenBookingFlowRequirement",
         "options":[
            "https://openactive.io/OnlinePrepayment",
            "https://openactive.io/OpenBookingIntakeForm",
            "https://openactive.io/OpenBookingAttendeeDetails",
            "https://openactive.io/OpenBookingApproval",
            "https://openactive.io/OpenBookingNegotiation",
            "https://openactive.io/OpenBookingMessageExchange"
         ]
      }
   }
}
